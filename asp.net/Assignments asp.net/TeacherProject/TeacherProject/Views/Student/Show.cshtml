@model TeacherProject.Models.Student

@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Student/List">Back To Students</a>
<div id="containershow">
    <h2>@Model.StudentFName @Model.StudentLName</h2>
    <p>Student Number: @Model.StudentNumber</p>
    <p>Date Enrolled: @Model.EnrolDate</p>
</div>
<a class="dellink" href="/Student/DeleteConfirm/@Model.StudentID">Delete Student</a>
<a class="navlink" href="/Student/Update/@Model.StudentID">Update Student</a>

