@model IEnumerable<TeacherProject.Models.Student>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>This page should be showing all information on all students.</p>

<div id="containerList">
    <h2>List of Students</h2>
    @foreach (var student in Model)
    {
        <div class="listitem row">
            <div class="col1-md-12"><a href="/Student/Show/@student.StudentID">@student.StudentFName @student.StudentLName</a></div>
        </div>
    }
</div>