@model TeacherProject.Models.Teacher

@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Teacher/List">Back To Teachers</a>
<div id="containershow">
    <h2>@Model.TeacherFName @Model.TeacherLName</h2>
    <p>Employee Number: @Model.EmployeeNumber</p>
    <p>Date Hired: @Model.HireDate</p>
    <p>Salary: @Model.Salary</p>
</div>
<a class="dellink" href="/Teacher/DeleteConfirm/@Model.TeacherID">Delete Teacher</a>

