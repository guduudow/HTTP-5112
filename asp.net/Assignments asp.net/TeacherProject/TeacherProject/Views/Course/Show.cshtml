@model TeacherProject.Models.Course

@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Course/List">Back To Course</a>
<div id="containershow">
    <h2>@Model.ClassCode @Model.ClassName</h2>
    <p>Teacher ID: @Model.TeacherID</p>
    <p>Start Date: @Model.StartDate</p>
    <p>Finish Date: @Model.FinishDate</p>
</div>
<a class="dellink" href="/Course/DeleteConfirm/@Model.ClassID">Delete Course</a>
<a class="navlink" href="/Course/Update/@Model.ClassID">Update Course</a>


